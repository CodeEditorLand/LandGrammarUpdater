{
	"name": "vscode-grammar-updater",
	"version": "0.0.1",
	"private": false,
	"description": "Utility to update TextMate grammars that are part of VSCode language extensions",
	"homepage": "https://github.com/CodeEditorLand/Foundation#readme",
	"bugs": {
		"url": "https://github.com/CodeEditorLand/Foundation/issues"
	},
	"repository": {
